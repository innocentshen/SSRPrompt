VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# 设置访问密码 - 默认密码是 admin123，请在部署前修改
VITE_APP_PASSWORD=admin123

# 本地 MySQL 代理服务器（新增）
# 如果配置了 VITE_MYSQL_PROXY_URL，将使用本地 Express 服务器
# 否则回退到 Supabase Edge Function
VITE_MYSQL_PROXY_URL=http://localhost:3001/api/mysql-proxy
VITE_MYSQL_PROXY_API_KEY=your_secure_api_key_here

# ========== Demo 空间配置 ==========
# Demo 空间允许用户无需配置数据库即可快速体验
# 需要配置以下环境变量以启用 Demo 模式

# Demo 数据库类型：supabase 或 mysql
VITE_DEMO_DB_PROVIDER=supabase

# Demo Supabase 配置（当 VITE_DEMO_DB_PROVIDER=supabase 时使用）
VITE_DEMO_SUPABASE_URL=your_demo_supabase_url
VITE_DEMO_SUPABASE_ANON_KEY=your_demo_supabase_anon_key

# Demo MySQL 配置（当 VITE_DEMO_DB_PROVIDER=mysql 时使用）
# VITE_DEMO_MYSQL_HOST=your_demo_mysql_host
# VITE_DEMO_MYSQL_PORT=3306
# VITE_DEMO_MYSQL_DATABASE=your_demo_database
# VITE_DEMO_MYSQL_USER=your_demo_user
# VITE_DEMO_MYSQL_PASSWORD=your_demo_password

# Demo 空间设置页密码（防止误操作）
VITE_DEMO_SETTINGS_PASSWORD=babamama2222@
